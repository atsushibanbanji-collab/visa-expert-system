    {
      "id": 19,
      "description": "Individual L Requirements",
      "conditions": {
        "type": "AND",
        "conditions": [
          "overseas_group_1_year_in_3",
          "meets_manager_staff_conditions_individual",
          "question_6_placeholder"
        ]
      },
      "conclusion": "meets_individual_l_conditions"
    },
    {
      "id": 20,
      "description": "Individual L Manager",
      "conditions": {
        "type": "AND",
        "conditions": [
          "management_experience_2_years_total",
          "us_role_managerial",
          "college_graduates_subordinates_2"
        ]
      },
      "conclusion": "individual_l_manager_conditions"
    },
    {
      "id": 21,
      "description": "Individual L Staff",
      "conditions": {
        "type": "AND",
        "conditions": [
          "has_specialized_knowledge",
          "us_role_requires_specialized_knowledge"
        ]
      },
      "conclusion": "individual_l_staff_conditions"
    },
    {
      "id": 21.5,
      "description": "Individual L Position Types",
      "conditions": {
        "type": "OR",
        "conditions": [
          "individual_l_manager_conditions",
          "individual_l_staff_conditions"
        ]
      },
      "conclusion": "meets_manager_staff_conditions_individual"
